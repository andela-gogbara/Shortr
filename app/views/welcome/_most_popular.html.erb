<div class="card blue-grey darken-1">
  <div class="card-content white-text">
    <span class="card-title">Popular Links</span>
    <% @most_popular.each do |link| %>
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title"><%= link.title%></span>
        <p><%= root_url + link.short_url %></p>
        <p>Clicks: <%= link.visit_count %></p>
        <p><%= time_ago_in_words(link.created_at) %></p>
      </div>
    </div>
    <% end %>
</div>
</div>
