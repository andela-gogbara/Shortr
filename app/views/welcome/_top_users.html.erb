<div class="card blue-grey darken-1">
  <div class="card-content white-text">
    <span class="card-title">Top Users</span>
    <% @top_users.each do |user|%>
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title"><%= user.name%></span>

        <p>Links Count: <%= user.links.count %></p>
      </div>
    </div>
    <% end %>
</div>
</div>
