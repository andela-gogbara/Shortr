<h1>User Page</h1>
<h3><%= @user.name %></h3>
  <div class="row">
    <div class="col l6">
      <ul class="collection">
        <% @user.links.each do |link|%>
           <div class="card blue-grey darken-1">
             <div class="card-content white-text">
               <span class="card-title"><%= link_to link.title, link_path(link), remote: true%></span>
               <p><%= link.full_url %></p>
               <p><%= link.short_url %></p>
             </div>
             <div class="card-action">
               <a href="#">This is a link</a>
               <a href="#">This is a link</a>
             </div>
           </div>
           <% end %>
    </ul>
    </div>
    <div class="col l6">
      <%= flash[:update_success] %>
      <%= flash[:link_exist] %>
      <%= flash[:link_deleted] %>
      <%= flash[:error] %>
      <div class="row" id="show-link">

        <%# render "links/show_link"%>
      </div>
    </div>
  </div>
