<h1 class="login-text">Login</h1>
<div class="container">
  <%= form_tag sessions_path do %>
  <div class="row">
  <div class="input-field col s12">
    <%= email_field_tag :email %>
    <%= label_tag :email %>
  </div>
</div>
  <div class="row">
  <div class="input-field col s12">
    <%= password_field_tag :password %>
    <%= label_tag :password %>
  </div>
</div>
  <div class="row">
    <%= submit_tag "Sign In" %>
</div>
  <% end %>
</div>
